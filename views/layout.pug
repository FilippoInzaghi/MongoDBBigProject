doctype html
mixin item(title, url)
  li
    a(href=url)=title

html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    ul
      li
        a(href="/") Main Page 
      li
        a(href="/news") News 
      li
        a(href="/quiz") Quiz
      li
        a(href="/admin") Admin  
    block content
